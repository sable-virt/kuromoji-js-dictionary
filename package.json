{
  "name": "kuromoji-js-dictionary",
  "version": "1.0.0",
  "description": "kuromoji.js dictionary generator",
  "main": "index.js",
  "scripts": {
    "start": "node --max-old-space-size=8192 build-dict.js",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -w -s -r 0",
    "xz": "xz -dk ./xz/*.xz",
    "euc": "find ./xz -name '*.csv' -exec nkf -e --overwrite {} \\;",
    "tar": "find ./xz -name '*.csv' -exec sh -c 'tar -Jcvf ./dict/neologd/$(basename '{}').tar.xz {}' \\;",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "dict": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontainer/kuromoji-js-dictionary.git"
  },
  "keywords": [
    "kuromoji.js",
    "dictionary"
  ],
  "author": "frontainer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frontainer/kuromoji-js-dictionary/issues"
  },
  "homepage": "https://github.com/frontainer/kuromoji-js-dictionary#readme",
  "dependencies": {
    "decompress": "^4.2.0",
    "decompress-tarxz": "^2.1.1",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "iconv-lite": "^0.4.19",
    "kuromoji": "^0.1.2",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.8",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.3.21"
  }
}
